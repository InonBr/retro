<div class="navbar navbar-expand-sm navbar-light navbar-lewagon fixed-top">
  <a class="navbar-brand" href="/gadgets">
    <img src="https://static.thenounproject.com/png/2421282-200.png" />
RetroTech</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item">
        <div class="d-flex justify-content-between">


            <%= link_to  new_gadget_path , class: 'nav-link upload-nav-link' do  %>
            Upload
              <img src="https://static.thenounproject.com/png/1396163-200.png" alt="add item">
        <% end  %>

        </div>
      </li>

      <% if user_signed_in? %>
      <li class="nav-item dropdown">
        <img class="avatar dropdown-toggle" id="navbarDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" src="https://cdn2.iconfinder.com/data/icons/audio-16/96/user_avatar_profile_login_button_account_member-512.png" />
        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
          <%= link_to "Dashboard", dashboard_path, class: 'dropdown-item' %>
          <%= link_to "Log out", destroy_user_session_path, method: :delete, class: 'logout-btn dropdown-item'%>
        </div>
      </li>
      <% else %>
      <%= link_to 'Sign Up', '/users/sign_up', class: 'sign-up' %>
      <%= link_to 'Login', '/users/sign_in', class: 'login'%>
      <% end %>
    </ul>
  </div>
</div>



